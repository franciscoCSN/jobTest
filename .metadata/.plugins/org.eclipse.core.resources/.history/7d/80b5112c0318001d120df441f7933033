public class App 
{
    public static void main( String[] args )
    {
        
//Questão 1)
   int indice = 13, soma = 0, k = 0;
   while(k < indice) {
	   k=k+1;
	   soma = soma + k;
   }
   System.out.printf("Finto processamento o valor de Soma é: %d%n", soma);
   

    	
    	
    	
    }
}
